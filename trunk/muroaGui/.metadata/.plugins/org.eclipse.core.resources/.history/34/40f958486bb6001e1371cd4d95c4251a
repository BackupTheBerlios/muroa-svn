#include "muroaserver.h"
#include "chwtest.h"

#include <QtGui>
#include <QApplication>
#include <QDebug>


int main(int argc, char *argv[])
{
    int retval;
    QApplication a(argc, argv);
    muroaserver w;

    try
    {
        w.show();
        retval = a.exec();
    }
    catch(...)
    {
        qDebug() << QSTring("got exception");
        retval = 32;
    }

    return retval;
}
