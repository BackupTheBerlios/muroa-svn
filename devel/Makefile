		cgpspos.h

nema.o: nema.cpp nema.h \
		cgpspos.h

cgpspos.o: cgpspos.cpp cgpspos.h

cnavapp.o: cnavapp.cpp cnavapp.h \
		cgpspos.h \
		nav.h \
		nema.h \
		nav_gui.h

nav_gui.h: nav_gui.ui 
	$(UIC) nav_gui.ui -o nav_gui.h

nav_gui.cpp: nav_gui.h nav_gui.ui 
	$(UIC) nav_gui.ui -i nav_gui.h -o nav_gui.cpp

nav_gui.o: nav_gui.cpp nav_gui.h

moc_nema.o: moc_nema.cpp  nema.h cgpspos.h

moc_cnavapp.o: moc_cnavapp.cpp  cnavapp.h cgpspos.h \
		nav.h \
		nema.h \
		nav_gui.h

moc_nav_gui.o: moc_nav_gui.cpp  nav_gui.h 

moc_nema.cpp: $(MOC) nema.h
	$(MOC) nema.h -o moc_nema.cpp

moc_cnavapp.cpp: $(MOC) cnavapp.h
	$(MOC) cnavapp.h -o moc_cnavapp.cpp

moc_nav_gui.cpp: $(MOC) nav_gui.h
	$(MOC) nav_gui.h -o moc_nav_gui.cpp

